{
    "name": "laraveljutsu/laravel-artifact",
    "description": "Artifact â€” Lightweight package that just handles basic media management with no fuss.",
    "type": "library",
    "keywords": ["laravel", "artifact", "media", "management"],
    "license": "MIT",
    "authors": [
        {
            "name": "ludoguenet",
            "email": "ludovicguenet@gmx.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.4",
        "laravel/pint": "^1.16",
        "phpstan/phpstan": "^1.11",
        "nunomaduro/larastan": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "LaravelJutsu\\Artifact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelJutsu\\Artifact\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelJutsu\\Artifact\\ArtifactServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/pint --parallel",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=1G",
        "test": "vendor/bin/pest --parallel",
        "qa": [
            "@composer format",
            "@composer analyse",
            "@composer test"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "version": "dev-main"
}
